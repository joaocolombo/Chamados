@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Sistemas Bischoff Group</title>

    <environment names="Development">
        <link rel="apple-touch-icon" href="http://static.bischoffgroup.com.br/resources/images/apple-touch-icon.png">
        <link rel="shortcut icon" href="http://static.bischoffgroup.com.br/resources/images/favicon.ico">

        <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/css/fonts.bundle.css">
        <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/css/base.bundle.css">
        <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/css/plugins.bundle.css">
    </environment>
    <environment names="Staging,Production">
        <link rel="apple-touch-icon" href="http://static.bischoffgroup.com.br/resources/images/apple-touch-icon.png">
        <link rel="shortcut icon" href="http://static.bischoffgroup.com.br/resources/images/favicon.ico">

        <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/css/fonts.bundle.css">
        <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/css/base.bundle.css">
        <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/css/plugins.bundle.css">
    </environment>
    @RenderSection("pluginsStyle", false)

    @RenderSection("customStyle", false)
    <!--[if lt IE 9]>
        <script src="http://static.bischoffgroup.com.br/resources/plugins/html5shiv/html5shiv.min.js"></script>
    <![endif]-->
    <!--[if lt IE 10]>
        <script src="http://static.bischoffgroup.com.br/resources/plugins/media-match/media.match.min.js"></script>
        <script src="http://static.bischoffgroup.com.br/resources/plugins/respond/respond.min.js"></script>
    <![endif]-->

    <script src="http://static.bischoffgroup.com.br/resources/plugins/breakpoints/breakpoints.js"></script>
    <script>
        Breakpoints();
    </script>

</head>
<body>

    @Html.Partial("_Header")
    @Html.Partial("_Menu")

    @RenderBody()

    @Html.Partial("_Footer")
    


<environment names="Development">
    <script src="http://static.bischoffgroup.com.br/resources/plugins/base.bundle.js"></script>
    <script src="http://static.bischoffgroup.com.br/resources/plugins/core.bundle.js"></script>

    @RenderSection("plugins", false)

    <script src="http://static.bischoffgroup.com.br/resources/js/scripts.bundle.js"></script>

    <script>
        (function (document, window, $) {
            'use strict';
            var Site = window.Site;
            $(document).ready(function () {
                Site.run();
                AppContacts.run();
            });
        })(document, window, jQuery);
    </script>


    <script>
        var onSuccess = function () {
            window.$.validator.unobtrusive.parse("form");
            var site = Site.getInstance();
            site.initializePlugins();
        }
    </script>
</environment>

    <environment names="Staging,Production">
        <script src="http://static.bischoffgroup.com.br/resources/plugins/base.bundle.js"></script>
        <script src="http://static.bischoffgroup.com.br/resources/plugins/core.bundle.js"></script>

        @RenderSection("plugins", false)

        <script src="http://static.bischoffgroup.com.br/resources/js/scripts.bundle.js"></script>

        <script>
                (function (document, window, $) {
                    'use strict';
                    var Site = window.Site;
                    $(document).ready(function () {
                        Site.run();
                        AppContacts.run();
                    });
                })(document, window, jQuery);
        </script>


        <script>
            var onSuccess = function () {
                window.$.validator.unobtrusive.parse("form");
                var site = Site.getInstance();
                site.initializePlugins();
            }
        </script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
