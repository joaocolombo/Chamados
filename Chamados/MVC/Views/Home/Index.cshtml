@model List<Domain.Entities.Chamado>
<div class="page">
    <div class="page-header">
        <h1 class="page-title mb-10">Chamados</h1>
    </div>
    <div class="page-content">
        <div class="container container-fluid">
            <div class="row justify-content-center">
                <div class="col-md-8">
                    <div class="panel">
                        <div class="panel-heading">
                            <h3 class="panel-title">Lista de Chamados Abertos</h3>
                        </div>
                        <div class="panel-body">
                            <form autocomplete="off">
                                <div class=" table-responsive">
                                    <table class="table">
                                        <thead>
                                        <tr>
                                            <th>Numero Chamado</th>
                                            <th>Atendente</th>
                                            <th>Assunto</th>
                                            <th>Loja</th>
                                            <th>Nivel</th>
                                            <th>#</th>
                                        </tr>
                                        </thead>
                                        <tbody>


                                        @for (int i = 0; i < Model.Count; i++)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(model => model[i].Codigo)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(model => model[i].Atendente.Nome)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(model => model[i].Assunto)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(model => model[i].Filial.Nome)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(model => model[i].Atendente.Nivel)
                                                </td>
                                                <td>
                                                    <button class="btn-default">Abrir</button>
                                                </td>
                                            </tr>
                                        }


                                        </tbody>
                                    </table>
                                </div>
                            </form>
                            <div class="site-action">
                                <button type="button" data-url="@Url.Action("Novo", "Home")" data-toggle="slidePanel" class="btn-raised btn btn-success btn-floating waves-effect waves-classic">
                                    <i class="front-icon md-plus animation-scale-up" aria-hidden="true"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
