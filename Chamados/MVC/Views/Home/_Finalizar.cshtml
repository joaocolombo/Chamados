
@model MVC.ViewModel.FinalizarViewModel

@section pluginsStyle
{
    <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/plugins/select2/select2.css">

}
<div class="page">
    <div class="page-header">
        <h1 class="page-title mb-10">Chamados</h1>
    </div>
    <div class="page-content">
        <div class="container container-fluid">
            <div class="row justify-content-center">
                <div class="col-md-8">
                    <div class="panel">
                        <div class="panel-heading">
                            <h3 class="panel-title">Finalizar</h3>
                        </div>
                        <div class="panel-body">
                            <form asp-action="Finalizar" asp-controller="Home" method="post">
                                <div hidden="">
                                    <input asp-for="NomeAtendente" Value="@Model.NomeAtendente" />
                                    <input asp-for="Id" Value="@Model.Id" />
                                </div>
                                <h4 class="panel-title">Apos finalizar, o Chamado não podera mais ser alterado, você tem certeza que deseja finaliza-lo?</h4>
                                <div class="text-right">
                                    <button type="submit" class="btn btn-lg btn-primary waves-effect waves-light">Finalizar</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section plugins
{
    <script src="http://static.bischoffgroup.com.br/resources/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="http://static.bischoffgroup.com.br/resources/plugins/select2/select2.js"></script>
}

