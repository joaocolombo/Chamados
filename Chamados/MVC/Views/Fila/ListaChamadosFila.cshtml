@model MVC.ViewModel.Fila.FilaViewModel


@section pluginsStyle
{
    <link rel="stylesheet" href="http://static.bischoffgroup.com.br/resources/plugins/select2/select2.css">
}
<div class="page">
    <div class="page-header">
        <h1 class="page-title mb-10">@Model.Descricao</h1>
    </div>
    <div class="page-content">
        <div class=" container-fluid">
            <div class="row justify-content-center">
                <div class="col-md-12">
                    <div class="panel">
                        <div class="panel-heading">
                            <h3 class="panel-title">Lista de Chamados do @Model.Descricao</h3>
                        </div>
                        <div class="panel-body">
                            <form autocomplete="off">
                                <div class=" table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th>Numero Chamado</th>
                                                <th>Solicitante</th>
                                                <th>Atendente</th>
                                                <th>Assunto</th>
                                                <th>Loja</th>
                                                <th>Nivel</th>
                                                <th>#</th>
                                            </tr>
                                        </thead>
                                        <tbody>



                                            @foreach (var chamado in Model.Lista)
                                            {
                                                <tr>
                                                    <td>
                                                        <label>@chamado.Codigo</label>
                                                    </td>
                                                    <td>
                                                        <label>@chamado.Solicitante</label>
                                                    </td>
                                                    <td>
                                                        <label>@chamado.Atendente.Nome</label>
                                                    </td>
                                                    <td>
                                                        <label>@chamado.Assunto</label>
                                                    </td>
                                                    <td>
                                                        <label>@chamado.Filial.Nome</label>
                                                    </td>
                                                    <td>
                                                        <label>@chamado.Atendente.Nivel</label>
                                                    </td>
                                                    <td>
                                                        <a class="btn-default" asp-controller="Fila" asp-action="VisualizarChamado" asp-route-id="@chamado.Codigo">Visualizar</a>
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section plugins
{
    <script src="http://static.bischoffgroup.com.br/resources/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="http://static.bischoffgroup.com.br/resources/plugins/select2/select2.js"></script>
}
